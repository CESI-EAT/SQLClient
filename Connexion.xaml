<Window x:Class="SQLClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SQLClient"
        mc:Ignorable="d"
        Title="Connexion" Height="800" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="322,126,0,0" TextWrapping="Wrap" Text="Nom d'utilisateur" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="322,168,0,0" TextWrapping="Wrap" Text="Mot de passe" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-0.896,-1.429"/>
        <Button Content="Se connecter" HorizontalAlignment="Left" Height="25" Margin="322,226,0,0" VerticalAlignment="Top" Width="120"/>
        <Image x:Name="CESIEAT_Logo" HorizontalAlignment="Left" Height="234" VerticalAlignment="Top" Width="228" Margin="59,68,0,0" Source="cesieat.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-62.937"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
    </Grid>
</Window>
